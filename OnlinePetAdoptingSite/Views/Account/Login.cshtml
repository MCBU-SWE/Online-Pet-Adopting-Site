@model dynamic

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}
<h2>Login</h2>
<form method="post">
    <label for="username">Username:</label>
    <input id="username" name="username" type="text" />
    <br />
    <label for="password">Password:</label>
    <input id="password" name="password" type="password" />
    <br />
    <button type="submit">Login</button>
</form>